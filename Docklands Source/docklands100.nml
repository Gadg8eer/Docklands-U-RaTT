// Road Type Selection 

roadtypetable {
 	ROAD, ELRD, YDRD, CBRD, CBRE, PVRD, PVRE, IRD1, ISRE, HAUL, ELHL
 }


// Tram Type Selection 


tramtypetable {
 	RAIL, ELRL, YDTR, CBTR, CBTE, PVTR, PVTE, NIAN, NIAE
 }





